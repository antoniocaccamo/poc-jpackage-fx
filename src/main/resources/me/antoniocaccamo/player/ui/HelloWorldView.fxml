<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.282" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.antoniocaccamo.player.ui.HelloWorldView">
  <children>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <TextField fx:id="locationURL" editable="true" HBox.hgrow="ALWAYS" />
            <Button fx:id="buttonGO" alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#buttonGOClicked" text="GO" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin></Button>
         </children>
         <padding>
            <Insets top="10.0" />
         </padding>
      </HBox>
    <WebView fx:id="webView" VBox.vgrow="ALWAYS" />

  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</VBox>
